      .breadcrumb {
        padding: 8px 11px;
        margin-bottom: 20px;
        list-style: none;
        background-color: transparent;
      }
      .breadcrumb > li {
        display: inline-block;
      }
      .breadcrumb > li + li:before {
        content: "/";
        padding: 0 5px;
        color: #999;
      }
      .breadcrumb > li > a {
        color: #0000EE;
        text-decoration: underline;
        cursor: pointer;
        
      }
      .breadcrumb > .active {
        color: #555;
      }